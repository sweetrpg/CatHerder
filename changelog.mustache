{{#tags}}
    <h3>{{name}}</h3>
    <ul>
        {{#commits}}
            <li>
                <a href="https://github.com/percivalalb/KittyTalents/commit/{{hashFull}}">{{{messageTitle}}}</a> - {{{authorName}}}
                <ul>
                    {{#messageBodyItems}}
                        <li>{{.}}</li>
                    {{/messageBodyItems}}
                </ul>
            </li>
        {{/commits}}
    </ul>
{{/tags}}
